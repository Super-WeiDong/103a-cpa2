<h1>My WatchList</h1>
<hr>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Category</th>
            <th>Runtime</th>
            <th>Language</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody></tbody>
    <% for (item of items) {
        const id = item["_id"];
         %>
      <tr class="mb-2">
        <td><Strong><%= item["name"] %></Strong></td>
        <td><%= item["category"] %></td>
        <td><%= item["runtime"] %></td>
        <td><%= item["language"] %></td>
        <td><button type="submit" class="btn"><a href="/watchlist/delete/<%= id %>">Delete</a></button></td>
    </tr>
    <% } %>
    </tbody>
</table>